<script>
document.addEventListener('DOMContentLoaded', function() {
jQuery(function($){
$(document).scroll(function() {
var y = $(this).scrollTop();
if (y > 175) { /* change this value here to make it show up at your desired scroll
location. */
$('#header-home').addClass('headerappear');
$('#header-inner').addClass('headerappear');
} else {
$('#header-home').removeClass('headerappear');
$('#header-inner').removeClass('headerappear');
}
});});});
