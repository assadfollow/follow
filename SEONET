		<script>

// register an event listene
document.addEventListener('DOMContentLoaded', function(){

// returns a live HTMLCollection of elements with the given tag name.
var getAllAttribute = document.getElementsByTagName('a');
    
// loop into the collection and add the event listener to each element
    for (var item = 0; item < getAllAttribute.length; item++) {
			
			  // remove var
        var getElement = getAllAttribute[item];

        // add the event listener to each element
        getElement.onclick = function(event) {

            // prevent the default action of the event
            var eachElement = event.currentTarget;

            // get the href attribute of the element
            // if(eachElement.href.includes('web.zid.sa/register')){
                event.preventDefault();
        
                //let urlParams = new URLSearchParams(urls[1]);
                var urlParams = window.location.href;
                var urlsParams = urlParams.split('?');

                // get the value of the parameter
                if(urlsParams.length > 1) {
                    window.location.href = (eachElement.href.includes('?')) ? (eachElement.href+'&'+urlsParams[1]) : (eachElement.href+'?'+urlsParams[1]);
                } else {
                    window.location.href = eachElement.href;
                }
                return false;
            //}

            return true;

        };
    }
});
</script>

